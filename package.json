  "devDependencies": {
    "@antfu/eslint-config": "^2.25.0",
    "@iconify/json": "^2.2.225",
    "@iconify/vue": "^4.1.2",
    "@tsconfig/node20": "^20.1.4",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.14.10",
    "@unocss/reset": "^0.62.2",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.57.0",
    "jsdom": "^24.1.0",
    "npm-run-all2": "^6.2.0",
    "sass": "^1.77.6",
    "typescript": "5.5.4",
    "unocss": "^0.62.3",
    "vite": "^5.4.2",
    "vite-plugin-css-injected-by-js": "^3.5.1",
    "vite-plugin-dts": "^3.9.1",
    "vitepress": "^1.3.4",
    "vitest": "^1.6.0",
    "vue-tsc": "^2.1.2"
  }
